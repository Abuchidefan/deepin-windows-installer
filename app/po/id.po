#: ../fontend/mainwindow.cpp:70
msgctxt "MainWindow|"
msgid "Username"
msgstr "Nama pengguna"

#: ../fontend/mainwindow.cpp:95
msgctxt "MainWindow|"
msgid "Password"
msgstr "Kata sandi"

#: ../fontend/mainwindow.cpp:118
msgctxt "MainWindow|"
msgid "Repeat Password"
msgstr "Ulang Kata sandi"

#: ../fontend/mainwindow.cpp:231
msgctxt "MainWindow|"
msgid "This operation will not affect any of your data. Please use it freely."
msgstr "Operasi ini tidak akan mempengaruhi data. Silakan gunakan secara bebas."

#: ../fontend/mainwindow.cpp:254
#, qt-format
msgctxt "MainWindow|"
msgid ""
"Please ensure that there is at least one%1 disk having more than 10GB free "
"space."
msgstr "Pastikan bahwa ada setidaknya satu%1 disk yang memiliki lebih dari 10GB ruang bebas."

#: ../fontend/mainwindow.cpp:263
msgctxt "MainWindow|"
msgid "Start"
msgstr "Mulai"

#: ../fontend/mainwindow.cpp:271
msgctxt "MainWindow|"
msgid "Exit"
msgstr "Keluar"

#: ../fontend/mainwindow.cpp:308
msgctxt "MainWindow|"
msgid "Being installed. Please wait... "
msgstr "Sedang diinstal. Silahkan tunggu... "

#: ../fontend/mainwindow.cpp:346
#, qt-format
msgctxt "MainWindow|"
msgid ""
"Installed Successfully. You need to restart your computer to experience %1."
msgstr "Penginstalan Berhasil. Anda perlu menyalakan kembali komputer untuk pengalaman %1."

#: ../fontend/mainwindow.cpp:354
msgctxt "MainWindow|"
msgid "Installation Failed."
msgstr "Penginstalan Gagal."

#: ../fontend/mainwindow.cpp:360
msgctxt "MainWindow|"
msgid "Restart Later"
msgstr "Nyalakan ulang Nanti"

#: ../fontend/mainwindow.cpp:363
msgctxt "MainWindow|"
msgid "Restart Now"
msgstr "Nyalakan ulang Sekarang"

#: ../fontend/mainwindow.cpp:389
msgctxt "MainWindow|"
msgid "Finished"
msgstr "Selesai"

#: ../fontend/mainwindow.cpp:531
#, qt-format
msgctxt "MainWindow|"
msgid "You have installed %1."
msgstr "Telah menginstal %1."

#: ../fontend/mainwindow.cpp:537
msgctxt "MainWindow|"
msgid "Cancel"
msgstr "Batal"

#: ../fontend/mainwindow.cpp:540
msgctxt "MainWindow|"
msgid "Uninstall"
msgstr "Hapus instalasi"

#: ../fontend/mainwindow.cpp:543
msgctxt "MainWindow|"
msgid "Reinstall"
msgstr "Instalasi Ulang"

#: ../fontend/mainwindow.cpp:555
msgctxt "MainWindow|"
msgid "Being uninstalled. Please wait... "
msgstr "Sedang menghapus instalasi. Silahkan tunggu..."

#: ../fontend/mainwindow.cpp:562
msgctxt "MainWindow|"
msgid "Uninstalled Successfully. "
msgstr "Penghapusan instalasi Berhasil."

#: ../fontend/mainwindow.cpp:569
msgctxt "MainWindow|"
msgid "Uninstallation Failed."
msgstr "Penghapusan Instalasi Gagal"

#: ../backend/backend.cpp:23
msgctxt "QObject|"
msgid "Deepin System Installer"
msgstr "Sistem Instalasi Deepin"

#: ../backend/utils.cpp:247 ../backend/utils.cpp:279
msgctxt "QObject|"
msgid "The first character must be in lower case."
msgstr "Karakter awal harus huruf kecil."

#: ../backend/utils.cpp:258 ../backend/utils.cpp:273
msgctxt "QObject|"
msgid "Username must comprise a~z, 0~9, - or _. "
msgstr "Nama pengguna harus mengandung a~z, 0~9, - or _. "

#: ../backend/utils.cpp:267
msgctxt "QObject|"
msgid "Username can not be empty. "
msgstr "Nama pengguna tidak boleh kosong."

#: ../backend/utils.cpp:286
msgctxt "QObject|"
msgid "The username has been used by system."
msgstr "Nama pengguna telah digunakan oleh sistem."

#: ../fontend/mainwindow.cpp:47
msgctxt "QObject|"
msgid "Password can not be empty."
msgstr "Kata sandi tidak boleh kosong."

#: ../fontend/mainwindow.cpp:48
msgctxt "QObject|"
msgid "The two passwords do not match."
msgstr "Kedua kata sandi tidak cocok."
