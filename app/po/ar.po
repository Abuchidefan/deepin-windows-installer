#: ../fontend/mainwindow.cpp:70
msgctxt "MainWindow|"
msgid "Username"
msgstr "اسم المستخدم"

#: ../fontend/mainwindow.cpp:95
msgctxt "MainWindow|"
msgid "Password"
msgstr "كلمة السر"

#: ../fontend/mainwindow.cpp:118
msgctxt "MainWindow|"
msgid "Repeat Password"
msgstr "أعد كلمة السر"

#: ../fontend/mainwindow.cpp:231
msgctxt "MainWindow|"
msgid "This operation will not affect any of your data. Please use it freely."
msgstr "لن تؤثر هذه العملية على أي من بياناتك, استخدمها بحرية."

#: ../fontend/mainwindow.cpp:254
#, qt-format
msgctxt "MainWindow|"
msgid ""
"Please ensure that there is at least one%1 disk having more than 10GB free "
"space."
msgstr "الرجاء التأكد أن لديك قرص صلب واحد على الأقل يحتوي على 10 جيجا مساحة فارغة."

#: ../fontend/mainwindow.cpp:263
msgctxt "MainWindow|"
msgid "Start"
msgstr "ابدأ"

#: ../fontend/mainwindow.cpp:271
msgctxt "MainWindow|"
msgid "Exit"
msgstr "اخرج"

#: ../fontend/mainwindow.cpp:308
msgctxt "MainWindow|"
msgid "Being installed. Please wait... "
msgstr "يتم التنصيب"

#: ../fontend/mainwindow.cpp:346
#, qt-format
msgctxt "MainWindow|"
msgid ""
"Installed Successfully. You need to restart your computer to experience %1."
msgstr "نجح التنصيب. تحتاج لإعادة تشغيل حاسبك للتجربة 1%."

#: ../fontend/mainwindow.cpp:354
msgctxt "MainWindow|"
msgid "Installation Failed."
msgstr "فشل التنصيب"

#: ../fontend/mainwindow.cpp:360
msgctxt "MainWindow|"
msgid "Restart Later"
msgstr "إعادة التشغيل لاحقا"

#: ../fontend/mainwindow.cpp:363
msgctxt "MainWindow|"
msgid "Restart Now"
msgstr "إعادة التشغيل الآن"

#: ../fontend/mainwindow.cpp:389
msgctxt "MainWindow|"
msgid "Finished"
msgstr "تمّ"

#: ../fontend/mainwindow.cpp:531
#, qt-format
msgctxt "MainWindow|"
msgid "You have installed %1."
msgstr "تم تنصيب 1%"

#: ../fontend/mainwindow.cpp:537
msgctxt "MainWindow|"
msgid "Cancel"
msgstr "إلغاء"

#: ../fontend/mainwindow.cpp:540
msgctxt "MainWindow|"
msgid "Uninstall"
msgstr "إلغاء التنصيب"

#: ../fontend/mainwindow.cpp:543
msgctxt "MainWindow|"
msgid "Reinstall"
msgstr "إعادة التنصيب"

#: ../fontend/mainwindow.cpp:555
msgctxt "MainWindow|"
msgid "Being uninstalled. Please wait... "
msgstr "يتم إلغاء التنصيب. الرجاء الانتظار..."

#: ../fontend/mainwindow.cpp:562
msgctxt "MainWindow|"
msgid "Uninstalled Successfully. "
msgstr "نجح إلغاء التنصيب"

#: ../fontend/mainwindow.cpp:569
msgctxt "MainWindow|"
msgid "Uninstallation Failed."
msgstr "فشل إلغاء التنصيب"

#: ../backend/backend.cpp:23
msgctxt "QObject|"
msgid "Deepin System Installer"
msgstr "برنامج تنصيب نظام ديبئن"

#: ../backend/utils.cpp:247 ../backend/utils.cpp:279
msgctxt "QObject|"
msgid "The first character must be in lower case."
msgstr "الحرف الأول يجب أن يكون صغيرا."

#: ../backend/utils.cpp:258 ../backend/utils.cpp:273
msgctxt "QObject|"
msgid "Username must comprise a~z, 0~9, - or _. "
msgstr "يجب أن يحوي اسم المستخدم على الأحرف من a إلى z , والأرقام من 0 إلى 9 , و - أو _ ."

#: ../backend/utils.cpp:267
msgctxt "QObject|"
msgid "Username can not be empty. "
msgstr "لا يمكن ترك اسم المستخدم فارغا."

#: ../backend/utils.cpp:286
msgctxt "QObject|"
msgid "The username has been used by system."
msgstr "تم استعمال اسم المستخدم من قبل النظام."

#: ../fontend/mainwindow.cpp:47
msgctxt "QObject|"
msgid "Password can not be empty."
msgstr "لا يمكن ترك كلمة السر فارغة."

#: ../fontend/mainwindow.cpp:48
msgctxt "QObject|"
msgid "The two passwords do not match."
msgstr "كلمتا المرور غير متطابقتين"
