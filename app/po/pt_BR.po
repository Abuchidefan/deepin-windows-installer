msgstr "Reinstalar"

#: ../fontend/mainwindow.cpp:555
msgctxt "MainWindow|"
msgid "Being uninstalled. Please wait... "
msgstr "Desinstalando. Por favor, aguarde..."

#: ../fontend/mainwindow.cpp:562
msgctxt "MainWindow|"
