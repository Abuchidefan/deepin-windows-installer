#: ../fontend/mainwindow.cpp:67
msgctxt "MainWindow|"
msgid "Username"
msgstr "Nazwa użytkownika"

#: ../fontend/mainwindow.cpp:92
msgctxt "MainWindow|"
msgid "Password"
msgstr "Hasło"

#: ../fontend/mainwindow.cpp:115
msgctxt "MainWindow|"
msgid "Repeat Password"
msgstr "Powtórz hasło"

#: ../fontend/mainwindow.cpp:228
msgctxt "MainWindow|"
msgid "This operation will not affect any of your data. Please use it freely."
msgstr "Ta operacja nie wpłynie w żaden sposób na Twoje dane. Można ją bezpiecznie używać."

#: ../fontend/mainwindow.cpp:251
#, qt-format
msgctxt "MainWindow|"
msgid ""
"Please ensure that there is at least one%1 disk having more than 10GB free "
"space."
msgstr "Prosimy się upewnić, że co najmniej jeden %1 dysk posiada przynajmniej 10GB wolnej przestrzeni."

#: ../fontend/mainwindow.cpp:260
msgctxt "MainWindow|"
msgid "Start"
msgstr "Rozpocznij"

#: ../fontend/mainwindow.cpp:268
msgctxt "MainWindow|"
msgid "Exit"
msgstr "Wyjdź"

#: ../fontend/mainwindow.cpp:300
msgctxt "MainWindow|"
msgid "Being installed. Please wait... "
msgstr "Rozpoczęto instalację. Prosimy czekać..."

#: ../fontend/mainwindow.cpp:338
#, qt-format
msgctxt "MainWindow|"
msgid ""
"Installed Successfully. You need to restart your computer to experience %1."
msgstr "Instalacja zakończona pomyślnie. Musisz teraz ponownie uruchomić swój komputer, aby móc doświadczać %1."

#: ../fontend/mainwindow.cpp:346
msgctxt "MainWindow|"
msgid "Installation Failed."
msgstr "Błąd instalacji."

#: ../fontend/mainwindow.cpp:352
msgctxt "MainWindow|"
msgid "Restart Later"
msgstr "Uruchom ponownie później"

#: ../fontend/mainwindow.cpp:355
msgctxt "MainWindow|"
msgid "Restart Now"
msgstr "Uruchom ponownie teraz"

#: ../fontend/mainwindow.cpp:381
msgctxt "MainWindow|"
msgid "Finished"
msgstr "Ukończono"

#: ../fontend/mainwindow.cpp:522
#, qt-format
msgctxt "MainWindow|"
msgid "You have installed %1."
msgstr "Zainstalowałeś %1."

#: ../fontend/mainwindow.cpp:528
msgctxt "MainWindow|"
msgid "Cancel"
msgstr "Anuluj"

#: ../fontend/mainwindow.cpp:531
msgctxt "MainWindow|"
msgid "Uninstall"
msgstr "Odinstaluj"

#: ../fontend/mainwindow.cpp:534
msgctxt "MainWindow|"
msgid "Reinstall"
msgstr "Ponownie zainstaluj"

#: ../fontend/mainwindow.cpp:546
msgctxt "MainWindow|"
msgid "Being uninstalled. Please wait... "
msgstr "Rozpoczęto odinstalowywanie. Prosimy czekać..."

#: ../fontend/mainwindow.cpp:553
msgctxt "MainWindow|"
msgid "Uninstalled Successfully. "
msgstr "Pomyślnie odinstalowano."

#: ../fontend/mainwindow.cpp:560
msgctxt "MainWindow|"
msgid "Uninstallation Failed."
msgstr "Błąd odinstalowywania."

#: ../backend/utils.cpp:247 ../backend/utils.cpp:279
msgctxt "QObject|"
msgid "The first character must be in lower case."
msgstr "Pierwszy znak musi być z małej litery."

#: ../backend/utils.cpp:258 ../backend/utils.cpp:273
msgctxt "QObject|"
msgid "Username must comprise a~z, 0~9, - or _. "
msgstr "Nazwa użytkownika musi składać się tylko ze znaków: a-z, 0-9, - lub _. "

#: ../backend/utils.cpp:267
msgctxt "QObject|"
msgid "Username can not be empty. "
msgstr "Nazwa użytkownika nie może być pusta."

#: ../backend/utils.cpp:286
#, qt-format
msgctxt "QObject|"
msgid "You can not use the reserved username: %1."
msgstr "Nie możesz użyć zarezerwowanej nazwy użytkownika: %1."

#: ../fontend/mainwindow.cpp:39
msgctxt "QObject|"
msgid "Password can not be empty. "
msgstr "Hasło nie może być puste."

#: ../fontend/mainwindow.cpp:40
msgctxt "QObject|"
msgid "The two passwords do not match."
msgstr "Oba hasła nie pasują do siebie."

#: ../main.cpp:41
msgctxt "QObject|"
msgid "Deepin Windows Installer"
msgstr "Instalator Deepin dla Windows"
