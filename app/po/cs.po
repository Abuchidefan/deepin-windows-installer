#: ../fontend/mainwindow.cpp:70
msgctxt "MainWindow|"
msgid "Username"
msgstr "Uživatelské jméno"

#: ../fontend/mainwindow.cpp:95
msgctxt "MainWindow|"
msgid "Password"
msgstr "Heslo"

#: ../fontend/mainwindow.cpp:118
msgctxt "MainWindow|"
msgid "Repeat Password"
msgstr "Zadejte heslo znovu"

#: ../fontend/mainwindow.cpp:231
msgctxt "MainWindow|"
msgid "This operation will not affect any of your data. Please use it freely."
msgstr "Tato operace neovlivní žádná z vašich dat. Používejte dle chuti."

#: ../fontend/mainwindow.cpp:254
#, qt-format
msgctxt "MainWindow|"
msgid ""
"Please ensure that there is at least one%1 disk having more than 10GB free "
"space."
msgstr "Zajistěte, prosím, že alespoň jeden %1 disk má více než 10 GB volného místa."

#: ../fontend/mainwindow.cpp:263
msgctxt "MainWindow|"
msgid "Start"
msgstr "Začít"

#: ../fontend/mainwindow.cpp:271
msgctxt "MainWindow|"
msgid "Exit"
msgstr "Ukončit"

#: ../fontend/mainwindow.cpp:308
msgctxt "MainWindow|"
msgid "Being installed. Please wait... "
msgstr "Instaluje se. Počkejte, prosím..."

#: ../fontend/mainwindow.cpp:346
#, qt-format
msgctxt "MainWindow|"
msgid ""
"Installed Successfully. You need to restart your computer to experience %1."
msgstr "Úspěšně nainstalováno. Abyste mohl %1 vyzkoušet, je potřeba počítač spustit znovu."

#: ../fontend/mainwindow.cpp:354
msgctxt "MainWindow|"
msgid "Installation Failed."
msgstr "Instalace se nezdařila."

#: ../fontend/mainwindow.cpp:360
msgctxt "MainWindow|"
msgid "Restart Later"
msgstr "Restartovat později"

#: ../fontend/mainwindow.cpp:363
msgctxt "MainWindow|"
msgid "Restart Now"
msgstr "Restartovat nyní"

#: ../fontend/mainwindow.cpp:389
msgctxt "MainWindow|"
msgid "Finished"
msgstr "Dokončeno"

#: ../fontend/mainwindow.cpp:531
#, qt-format
msgctxt "MainWindow|"
msgid "You have installed %1."
msgstr "Nainstaloval jste %1."

#: ../fontend/mainwindow.cpp:537
msgctxt "MainWindow|"
msgid "Cancel"
msgstr "Zrušit"

#: ../fontend/mainwindow.cpp:540
msgctxt "MainWindow|"
msgid "Uninstall"
msgstr "Odinstalovat"

#: ../fontend/mainwindow.cpp:543
msgctxt "MainWindow|"
msgid "Reinstall"
msgstr "Přeinstalovat"

#: ../fontend/mainwindow.cpp:555
msgctxt "MainWindow|"
msgid "Being uninstalled. Please wait... "
msgstr "Odinstalovává se. Počkejte, prosím..."

#: ../fontend/mainwindow.cpp:562
msgctxt "MainWindow|"
msgid "Uninstalled Successfully. "
msgstr "Odinstalace byla úspěšná."

#: ../fontend/mainwindow.cpp:569
msgctxt "MainWindow|"
msgid "Uninstallation Failed."
msgstr "Odinstalování se nezdařilo"

#: ../backend/backend.cpp:23
msgctxt "QObject|"
msgid "Deepin System Installer"
msgstr "Instalátor systému Deepin"

#: ../backend/utils.cpp:247 ../backend/utils.cpp:279
msgctxt "QObject|"
msgid "The first character must be in lower case."
msgstr "První písmeno musí být malé."

#: ../backend/utils.cpp:258 ../backend/utils.cpp:273
msgctxt "QObject|"
msgid "Username must comprise a~z, 0~9, - or _. "
msgstr "Uživatelské jméno se musí skládat z a~z, 0~9, - nebo _. "

#: ../backend/utils.cpp:267
msgctxt "QObject|"
msgid "Username can not be empty. "
msgstr "Uživatelské jméno nemůže být prázdné."

#: ../backend/utils.cpp:286
msgctxt "QObject|"
msgid "The username has been used by system."
msgstr "Uživatelské jméno bylo použito systémem."

#: ../fontend/mainwindow.cpp:47
msgctxt "QObject|"
msgid "Password can not be empty."
msgstr "Heslo nemůže být prázdné."

#: ../fontend/mainwindow.cpp:48
msgctxt "QObject|"
msgid "The two passwords do not match."
msgstr "Hesla se neshodují."
