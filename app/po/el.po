#: ../fontend/mainwindow.cpp:70
msgctxt "MainWindow|"
msgid "Username"
msgstr "Όνομα χρήστη"

#: ../fontend/mainwindow.cpp:95
msgctxt "MainWindow|"
msgid "Password"
msgstr "Κωδικός πρόσβασης"

#: ../fontend/mainwindow.cpp:118
msgctxt "MainWindow|"
msgid "Repeat Password"
msgstr "Επανάληψη Κωδικού"

#: ../fontend/mainwindow.cpp:231
msgctxt "MainWindow|"
msgid "This operation will not affect any of your data. Please use it freely."
msgstr "Η λειτουργία αυτή δεν θα επηρεάσει τα δεδομένα της. Χρησιμοποιήστε την ελεύθερα."

#: ../fontend/mainwindow.cpp:254
#, qt-format
msgctxt "MainWindow|"
msgid ""
"Please ensure that there is at least one%1 disk having more than 10GB free "
"space."
msgstr "Παρακαλώ σιγουρευτείτε ότι υπάρχει τουλάχιστον %1 δίσκος με παραπάνω από 10GB ελεύθερου χώρου."

#: ../fontend/mainwindow.cpp:263
msgctxt "MainWindow|"
msgid "Start"
msgstr "Έναρξη"

#: ../fontend/mainwindow.cpp:271
msgctxt "MainWindow|"
msgid "Exit"
msgstr "Έξοδος"

#: ../fontend/mainwindow.cpp:308
msgctxt "MainWindow|"
msgid "Being installed. Please wait... "
msgstr "Γίνεται εγκατάσταση. Παρακαλώ περιμένετε..."

#: ../fontend/mainwindow.cpp:346
#, qt-format
msgctxt "MainWindow|"
msgid ""
"Installed Successfully. You need to restart your computer to experience %1."
msgstr "Επιτυχής Εγκατάσταση. Πρέπει να επανεκκινήσετε τον υπολογιστή σας για να χρησιμοποιήσετε το %1."

#: ../fontend/mainwindow.cpp:354
msgctxt "MainWindow|"
msgid "Installation Failed."
msgstr "Αποτυχία Εγκατάστασης."

#: ../fontend/mainwindow.cpp:360
msgctxt "MainWindow|"
msgid "Restart Later"
msgstr "Επανεκκίνηση Αργότερα"

#: ../fontend/mainwindow.cpp:363
msgctxt "MainWindow|"
msgid "Restart Now"
msgstr "Επανεκκίνηση Τώρα"

#: ../fontend/mainwindow.cpp:389
msgctxt "MainWindow|"
msgid "Finished"
msgstr "Ολοκληρώθηκε"

#: ../fontend/mainwindow.cpp:531
#, qt-format
msgctxt "MainWindow|"
msgid "You have installed %1."
msgstr "Έχετε εγκαταστήσει το %1."

#: ../fontend/mainwindow.cpp:537
msgctxt "MainWindow|"
msgid "Cancel"
msgstr "Ακύρωση"

#: ../fontend/mainwindow.cpp:540
msgctxt "MainWindow|"
msgid "Uninstall"
msgstr "Απεγκατάσταση"

#: ../fontend/mainwindow.cpp:543
msgctxt "MainWindow|"
msgid "Reinstall"
msgstr "Επανεγκατάσταση"

#: ../fontend/mainwindow.cpp:555
msgctxt "MainWindow|"
msgid "Being uninstalled. Please wait... "
msgstr "Απεγκατάσταση. Παρακαλώ περιμένετε..."

#: ../fontend/mainwindow.cpp:562
msgctxt "MainWindow|"
msgid "Uninstalled Successfully. "
msgstr "Επιτυχής Απεγκατάσταση."

#: ../fontend/mainwindow.cpp:569
msgctxt "MainWindow|"
msgid "Uninstallation Failed."
msgstr "Αποτυχία Απεγκατάστασης."

#: ../backend/backend.cpp:23
msgctxt "QObject|"
msgid "Deepin System Installer"
msgstr "Βοηθός Εγκατάστασης Συστήματος Deepin"

#: ../backend/utils.cpp:247 ../backend/utils.cpp:279
msgctxt "QObject|"
msgid "The first character must be in lower case."
msgstr "Ο πρώτος χαρακτήρας πρέπει να είναι μικρός."

#: ../backend/utils.cpp:258 ../backend/utils.cpp:273
msgctxt "QObject|"
msgid "Username must comprise a~z, 0~9, - or _. "
msgstr "Το όνομα χρήστη πρέπει να περιλαμβάνει α~ω, 0~9, - ή _."

#: ../backend/utils.cpp:267
msgctxt "QObject|"
msgid "Username can not be empty. "
msgstr "Το όνομα χρήστη δεν μπορεί να είναι κενό."

#: ../backend/utils.cpp:286
msgctxt "QObject|"
msgid "The username has been used by system."
msgstr "Το όνομα χρήστη έχει χρησιμοποιηθεί από το σύστημα."

#: ../fontend/mainwindow.cpp:47
msgctxt "QObject|"
msgid "Password can not be empty."
msgstr "Ο κωδικός πρόσβασης δεν μπορεί να είναι κενός."

#: ../fontend/mainwindow.cpp:48
msgctxt "QObject|"
msgid "The two passwords do not match."
msgstr "Οι δύο κωδικοί δεν ταιριάζουν."
