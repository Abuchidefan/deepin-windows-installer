#: ../fontend/mainwindow.cpp:70
msgctxt "MainWindow|"
msgid "Username"
msgstr "Gebruikersnaam"

#: ../fontend/mainwindow.cpp:95
msgctxt "MainWindow|"
msgid "Password"
msgstr "Wachtwoord"

#: ../fontend/mainwindow.cpp:118
msgctxt "MainWindow|"
msgid "Repeat Password"
msgstr "Herhaal wachtwoord"

#: ../fontend/mainwindow.cpp:231
msgctxt "MainWindow|"
msgid "This operation will not affect any of your data. Please use it freely."
msgstr "Deze operatie zal geen gegevens van u beïnvloeden. U kan ze ongestoord gebruiken."

#: ../fontend/mainwindow.cpp:254
#, qt-format
msgctxt "MainWindow|"
msgid ""
"Please ensure that there is at least one%1 disk having more than 10GB free "
"space."
msgstr "Zorg ervoor dat er ten minste one%1 schijf met meer dan 10 GB vrije ruimte beschikbaar is. "

#: ../fontend/mainwindow.cpp:263
msgctxt "MainWindow|"
msgid "Start"
msgstr "Beginnen"

#: ../fontend/mainwindow.cpp:271
msgctxt "MainWindow|"
msgid "Exit"
msgstr "Beëindigen"

#: ../fontend/mainwindow.cpp:308
msgctxt "MainWindow|"
msgid "Being installed. Please wait... "
msgstr "Wordt geïnstalleerd. Even geduld a.u.b..."

#: ../fontend/mainwindow.cpp:346
#, qt-format
msgctxt "MainWindow|"
msgid ""
"Installed Successfully. You need to restart your computer to experience %1."
msgstr "Installatie geslaagd. Uw computer moet opnieuw opgestart worden om gebruik te maken van %1."

#: ../fontend/mainwindow.cpp:354
msgctxt "MainWindow|"
msgid "Installation Failed."
msgstr "Installatie mislukt."

#: ../fontend/mainwindow.cpp:360
msgctxt "MainWindow|"
msgid "Restart Later"
msgstr "Later opnieuw opstarten."

#: ../fontend/mainwindow.cpp:363
msgctxt "MainWindow|"
msgid "Restart Now"
msgstr "Nu opnieuw opstarten"

#: ../fontend/mainwindow.cpp:389
msgctxt "MainWindow|"
msgid "Finished"
msgstr "Gereed"

#: ../fontend/mainwindow.cpp:531
#, qt-format
msgctxt "MainWindow|"
msgid "You have installed %1."
msgstr "U heeft %1 geinstalleerd."

#: ../fontend/mainwindow.cpp:537
msgctxt "MainWindow|"
msgid "Cancel"
msgstr "Annuleren"

#: ../fontend/mainwindow.cpp:540
msgctxt "MainWindow|"
msgid "Uninstall"
msgstr "Deïnstalleren"

#: ../fontend/mainwindow.cpp:543
msgctxt "MainWindow|"
msgid "Reinstall"
msgstr "Opnieuw installeren"

#: ../fontend/mainwindow.cpp:555
msgctxt "MainWindow|"
msgid "Being uninstalled. Please wait... "
msgstr "Wordt verwijderd. Gelieve te wachten..."

#: ../fontend/mainwindow.cpp:562
msgctxt "MainWindow|"
msgid "Uninstalled Successfully. "
msgstr "Met succes verwijderd."

#: ../fontend/mainwindow.cpp:569
msgctxt "MainWindow|"
msgid "Uninstallation Failed."
msgstr "Verwijderen mislukt."

#: ../backend/backend.cpp:23
msgctxt "QObject|"
msgid "Deepin System Installer"
msgstr "Deepin systeeminstallatie"

#: ../backend/utils.cpp:247 ../backend/utils.cpp:279
msgctxt "QObject|"
msgid "The first character must be in lower case."
msgstr "Het eerste teken mag geen hoofdletter zijn."

#: ../backend/utils.cpp:258 ../backend/utils.cpp:273
msgctxt "QObject|"
msgid "Username must comprise a~z, 0~9, - or _. "
msgstr "De gebruikersnaam moet bestaan uit a~z, 0~9, - of _."

#: ../backend/utils.cpp:267
msgctxt "QObject|"
msgid "Username can not be empty. "
msgstr "Gebruikersnaam mag niet leeg zijn."

#: ../backend/utils.cpp:286
msgctxt "QObject|"
msgid "The username has been used by system."
msgstr "De gebruikersnaam werd gebruikt door het systeem."

#: ../fontend/mainwindow.cpp:47
msgctxt "QObject|"
msgid "Password can not be empty."
msgstr "Wachtwoord mag niet leeg zijn."

#: ../fontend/mainwindow.cpp:48
msgctxt "QObject|"
msgid "The two passwords do not match."
msgstr "De twee wachtwoorden komen niet overeen."
