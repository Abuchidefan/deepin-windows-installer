msgctxt "MainWindow|"
msgid "Uninstall"
msgstr "Deinstaliraj"

#: ../fontend/mainwindow.cpp:543
msgctxt "MainWindow|"
msgid "Reinstall"
msgstr "Ponovno instaliraj"

#: ../fontend/mainwindow.cpp:555
msgctxt "MainWindow|"
