#: ../fontend/mainwindow.cpp:67
msgctxt "MainWindow|"
msgid "Username"
msgstr "Имя пользователя"

#: ../fontend/mainwindow.cpp:92
msgctxt "MainWindow|"
msgid "Password"
msgstr "Пароль"

#: ../fontend/mainwindow.cpp:115
msgctxt "MainWindow|"
msgid "Repeat Password"
msgstr "Повторите пароль"

#: ../fontend/mainwindow.cpp:228
msgctxt "MainWindow|"
msgid "This operation will not affect any of your data. Please use it freely."
msgstr "Эта операция не повлияет на Ваши данные. Используйте её свободно."

#: ../fontend/mainwindow.cpp:251
#, qt-format
msgctxt "MainWindow|"
msgid ""
"Please ensure that there is at least one%1 disk having more than 10GB free "
"space."
msgstr "Убедитесь, что существует, хотя бы один%1 диск, с более чем 10 Гб свободного пространства."

#: ../fontend/mainwindow.cpp:260
msgctxt "MainWindow|"
msgid "Start"
msgstr "Старт"

#: ../fontend/mainwindow.cpp:268
msgctxt "MainWindow|"
msgid "Exit"
msgstr "Выход"

#: ../fontend/mainwindow.cpp:300
msgctxt "MainWindow|"
msgid "Being installed. Please wait... "
msgstr "Устанавливается. Подождите..."

#: ../fontend/mainwindow.cpp:338
#, qt-format
msgctxt "MainWindow|"
msgid ""
"Installed Successfully. You need to restart your computer to experience %1."
msgstr "Успешно установлено. Вам нужно перезагрузить компьютер для использования %1."

#: ../fontend/mainwindow.cpp:346
msgctxt "MainWindow|"
msgid "Installation Failed."
msgstr "Установка неудачна."

#: ../fontend/mainwindow.cpp:352
msgctxt "MainWindow|"
msgid "Restart Later"
msgstr "Перезагрузить позже"

#: ../fontend/mainwindow.cpp:355
msgctxt "MainWindow|"
msgid "Restart Now"
msgstr "Перезагрузить сейчас"

#: ../fontend/mainwindow.cpp:381
msgctxt "MainWindow|"
msgid "Finished"
msgstr "Завершено"

#: ../fontend/mainwindow.cpp:522
#, qt-format
msgctxt "MainWindow|"
msgid "You have installed %1."
msgstr "Вами установлено %1."

#: ../fontend/mainwindow.cpp:528
msgctxt "MainWindow|"
msgid "Cancel"
msgstr "Отменить"

#: ../fontend/mainwindow.cpp:531
msgctxt "MainWindow|"
msgid "Uninstall"
msgstr "Удалить"

#: ../fontend/mainwindow.cpp:534
msgctxt "MainWindow|"
msgid "Reinstall"
msgstr "Переустановить"

#: ../fontend/mainwindow.cpp:546
msgctxt "MainWindow|"
msgid "Being uninstalled. Please wait... "
msgstr "Удаляется. Подождите..."

#: ../fontend/mainwindow.cpp:553
msgctxt "MainWindow|"
msgid "Uninstalled Successfully. "
msgstr "Удаление успешно."

#: ../fontend/mainwindow.cpp:560
msgctxt "MainWindow|"
msgid "Uninstallation Failed."
msgstr "Удаление неудачно."

#: ../backend/utils.cpp:247 ../backend/utils.cpp:279
msgctxt "QObject|"
msgid "The first character must be in lower case."
msgstr "Первый символ должен быть в нижнем регистре."

#: ../backend/utils.cpp:258 ../backend/utils.cpp:273
msgctxt "QObject|"
msgid "Username must comprise a~z, 0~9, - or _. "
msgstr "Имя пользователя должно содержать a~z, 0~9, - или _. "

#: ../backend/utils.cpp:267
msgctxt "QObject|"
msgid "Username can not be empty. "
msgstr "Имя пользователя не может быть пустым."

#: ../backend/utils.cpp:286
#, qt-format
msgctxt "QObject|"
msgid "You can not use the reserved username: %1."
msgstr "Вы не можете использовать зарезервированное имя: %1."

#: ../fontend/mainwindow.cpp:39
msgctxt "QObject|"
msgid "Password can not be empty. "
msgstr "Пароль не может быть пустым."

#: ../fontend/mainwindow.cpp:40
msgctxt "QObject|"
msgid "The two passwords do not match."
msgstr "Пароли не совпадают."

